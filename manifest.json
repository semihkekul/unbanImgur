{
    "manifest_version": 2,
    "name": "unbanimgur",
    "version": "1.1",
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": [
        "*://imgur.com/*",
        "*://www.imgur.com/*",
        "*://i.imgur.com/*",
        "webRequest", "webRequestBlocking", "storage"
    ],
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "icons":
    {
       "128": "icon_128.png",
       "16": "icon_16.png",
       "48": "icon_48.png"
    }
 
  }