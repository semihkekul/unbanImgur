{
    "manifest_version": 2,
    "name": "Imgur Proxied",
    "version": "0.1",
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["jquery-3.3.1.min.js"]
      }
    ],
    "permissions": [
        "webRequest",
        "*://imgur.com/*",
        "*://i.imgur.com/*",
        "*://www.imgur.com/*",
        "webRequestBlocking"
      ],
    "browser_action": {
      "default_icon": "icon.png"
    },
    "icons":
    {
       "128": "icon.png",
       "16": "icon.png",
       "48": "icon.png"
    }
 
  }